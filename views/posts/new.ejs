<h1>New Posts</h1>

<form action="/posts" method="post" enctype="multipart/form-data">
    <input type="text" name="post[title]" placeholder="Title">
    <br>
    <input type="text" name="post[price]" placeholder="Price">
    <br>
    <div>
        <input type="file" name='images' accept="images/*" multiple>
    </div>
    <textarea name="post[description]" cols="30" rows="10" placeholder="Description"></textarea>
    <br>
    <input type="text" name="post[location]" placeholder="Location">
    <br>
    <input type="submit">
</form>